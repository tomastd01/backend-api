<h1>Vista de Productos</h1>
    <table>
        <tr>
            <th>Producto</th>
            <th>Precio</th>
            <th>Foto</th>
        </tr>
        {{#each products}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.price}}</td>
                <td>
                    <img src={{this.thumbnail}} alt="" width="80px">
                </td>
            </tr>
        {{/each}}
    </table>
    <button>
        <a href="/">Ir al formulario</a>
    </button>